<?xml version="1.0" encoding="UTF-8"?>

<project name="SafeCity" default="default" basedir=".">
    <description>Builds, tests, and runs the project SafeCity.</description>
    <import file="nbproject/build-impl.xml"/>
    
    <target name="-post-jar">
        <jar destfile="${dist.jar}" update="true">
            <manifest>
                <attribute name="Built-By" value="jayfella"/>
            </manifest>
        </jar>
        
        <copy file="${dist.jar}" todir="D:\\BukkitTest\\Plugins\\"/>
	
    </target>
    
</project>
